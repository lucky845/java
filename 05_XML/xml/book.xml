<?xml version="1.0" encoding="UTF-8"?>
<!--
    <?xml version="1.0" encoding="UTF-8"?>
    以上内容就是xml文件的声明
    version="1.0"       version 表示xml的版本
    encoding="UTF-8"    encoding 表示xml文件本身的编码
 -->
<books><!-- books 表示多个图书信息 -->
    <book sn="SN123444684"> <!-- book标签描述一本图书   sn属性描述 的是图书 的编号  -->
        <name>时间简史</name> <!-- name标签表示书名 -->
        <author>霍金</author> <!-- author标签表示作者 -->
        <price>75</price> <!-- price标签表示图书价格 -->
    </book>
    <book sn="SN1265164684"> <!-- book标签描述一本图书   sn属性描述 的是图书 的编号  -->
        <name>java从入门到放弃</name> <!-- name标签表示书名 -->
        <author>Java</author> <!-- author标签表示作者 -->
        <price>9.9</price> <!-- price标签表示图书价格 -->
    </book>
    <book sn="SN1265164684" name="辟邪剑法" author="无名" price="9999999"/><!-- 单标签 -->
    <![CDATA[ 这里可以把你输入的字符原样显示，不会解析 xml ]]> <!-- <![CDATA[]]>  -->
</books>